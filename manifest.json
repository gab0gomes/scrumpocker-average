{
  "name": "ScrumPoker-Online Average Calculator",
  "description": "Calculates the average of current estimates",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.scrumpoker-online.org/en/room/*"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/favicon-16x16.png",
      "32": "/images/favicon-32x32.png",
      "48": "/images/favicon-48x48.png",
      "128": "/images/favicon-128x128.png"
    }
  },
  "icons": {
    "16": "/images/favicon-16x16.png",
    "32": "/images/favicon-32x32.png",
    "48": "/images/favicon-48x48.png",
    "128": "/images/favicon-128x128.png"
  }
}
